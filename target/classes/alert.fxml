<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="100.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AlertController">
   <children>
      <ButtonBar layoutY="60.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="60.0">
        <buttons>
          <Button fx:id="btn_yes" alignment="CENTER" mnemonicParsing="false" text="Ja" />
            <Button fx:id="btn_no" alignment="CENTER" layoutX="135.0" layoutY="18.0" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" text="Nein" />
        </buttons>
      </ButtonBar>
      <Label layoutX="14.0" layoutY="14.0" text="Label" fx:id="l_text"/>
   </children>
</AnchorPane>
